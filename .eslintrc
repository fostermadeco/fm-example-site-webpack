{
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "semi": 2,
        "quotes": ["error", "single", { "allowTemplateLiterals": true }],
        "no-console": 0,
        "no-unused-vars": 1,
        "indent": [ 2, 4 ],
        "max-len": 1,
        "no-underscore-dangle": 0,
        "no-use-before-define": 1,
        "object-shorthand": 1,
        "no-param-reassign": 0,
        "import/prefer-default-export": 1,
        "prefer-template": 1
    }
}